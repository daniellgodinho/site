{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/**",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/loadermenu",
      "destination": "/api/loadermenu.js"
    },
    {
      "source": "/loaderfarm",
      "destination": "/api/loaderfarm.js"
    },
    {
      "source": "/dnmenu",
      "destination": "/api/raw/[slug].js?slug=dnmenu"
    },
    {
      "source": "/dnfarm",
      "destination": "/api/raw/[slug].js?slug=dnfarm"
    },
    {
      "source": "/usermenu",
      "destination": "/api/usermenu.js"
    },
    {
      "source": "/userfarm",
      "destination": "/api/userfarm.js"
    },
    {
      "source": "/api/generate-link",
      "destination": "/api/generate-link.js"
    },
    {
      "source": "/api/redeem/:path*",
      "destination": "/api/redeem.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(dnmenu|dnfarm|usermenu|userfarm|loadermenu|loaderfarm)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
}